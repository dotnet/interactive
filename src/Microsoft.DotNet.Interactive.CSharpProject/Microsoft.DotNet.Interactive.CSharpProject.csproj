    <None Remove="TestResults\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="C:\Users\brett\.nuget\packages\fsharp.core\6.0.1\contentFiles\any\netstandard2.1\FSharp.Core.xml" />
  </ItemGroup>

