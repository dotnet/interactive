{
    "indentationRules": {
        "increaseIndentPattern": "^.*\\{[^}\"']*$",
        "decreaseIndentPattern": "^(.*\\*/)?\\s*\\}.*$"
    },
    "comments": {
        "lineComment": "#",
        "blockComment": [
            "<#",
            "#>"
        ],
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
    ],
}